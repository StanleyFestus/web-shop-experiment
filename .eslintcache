\n  const [state, dispatch] = useReducer(reducer, initialState);\n  \n  const openSidebar = () => {\n    dispatch({\n      type: SIDEBAR_OPEN\n    })\n  }\n\n  const closeSidebar = () => {\n    dispatch({\n      type: SIDEBAR_CLOSE,\n    })\n  }\n\n  const fetchProducts = async(url) => {\n    dispatch({type: GET_PRODUCTS_BEGIN})\n    try {\n      const response = await axios.get(url);\n      const products = response.data;\n      dispatch({\n        type: GET_PRODUCTS_SUCCESS,\n        payload: products\n      })\n    } catch (error){\n      dispatch({\n        type: GET_PRODUCTS_ERROR\n      })\n    }\n  }\n\n  const fetchSingleProduct = async (url) => {\n    dispatch({ type: GET_SINGLE_PRODUCT_BEGIN })\n    try {\n      const response = await axios.get(url)\n      const singleProduct = response.data\n      dispatch({ \n        type: GET_SINGLE_PRODUCT_SUCCESS, \n        payload: singleProduct })\n    } catch (error) {\n      dispatch({ \n        type: GET_SINGLE_PRODUCT_ERROR })\n    }\n  }\n\n  const str = () => {\n    return \"Hello\"\n  }\n\n  useEffect(() => {\n    fetchProducts(url)\n  })\n\n  return (\n    <ProductsContext.Provider value={\n      state,\n      openSidebar,\n      closeSidebar,\n      fetchSingleProduct,\n      str\n    }>\n      {children}\n    </ProductsContext.Provider>\n  )\n}","/home/kelewe/Projects/web-store-demo-app/src/reducers/products_reducer.js",[],"/home/kelewe/Projects/web-store-demo-app/src/components/NavBar/NavBar.js",[],"/home/kelewe/Projects/web-store-demo-app/src/pages/Home/index.js",[],"/home/kelewe/Projects/web-store-demo-app/src/components/NavBar/index.js",["39","40"],{"ruleId":"41","severity":1,"message":"42","line":2,"column":10,"nodeType":"43","messageId":"44","endLine":2,"endColumn":16},{"ruleId":"41","severity":1,"message":"45","line":4,"column":10,"nodeType":"43","messageId":"44","endLine":4,"endColumn":25},{"ruleId":"46","severity":1,"message":"47","line":85,"column":12,"nodeType":"48","messageId":"49","endLine":85